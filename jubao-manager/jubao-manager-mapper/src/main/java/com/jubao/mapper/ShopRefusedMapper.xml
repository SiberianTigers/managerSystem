<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jubao.mapper.ShopRefusedMapper">
<!-- 添加信息 -->
<insert id="addShopRefused" parameterType="shopRefused">
    
    INSERT INTO `jubao`.`shop_refused` (
  `shopid`,
  `content`,
  `createDate`
)
VALUES
  (
    #{shopid},
    #{content},
    #{createDate}
  );
  
</insert>


</mapper>